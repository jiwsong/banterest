<div id="rants", class="transitions-enabled">
  <% @rants.each do |rant| %>
    <div class="box panel panel-default">
      <%= link_to image_tag(rant.image.url(:thumb)), rant %>
      <div class="panel-body">
        <%= rant.description %>
        <br>
        <%= rant.user.email if rant.user %>
        <br>
        <% if rant.user == current_user %>
          <div class="actions">
            <%= link_to 'Edit', edit_rant_path(rant) %>
            | <%= link_to 'Delete', rant, method: :delete, data: { confirm: 'Are you sure?' } %>
          </div>
        <% end %>
      </div>
    </div>
  <% end %>
</div>
